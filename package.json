{
  "name": "nextjs-mdx-blog-starter",
  "description": "Starter for creating a Next.js powered blog with content authored in MDX.",
  "version": "0.1.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "deploy": "git push origin main && vercel --prod"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mdx-js/loader": "^1.6.16",
    "@mdx-js/runtime": "^1.6.16",
    "@next/mdx": "^9.5.1",
    "disqus-react": "^1.0.9",
    "gray-matter": "^4.0.2",
    "mdx-embed": "^0.0.9",
    "next": "^9.5.1",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.9",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-ga": "^3.1.2",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.0.1",
    "react-transition-group": "^4.4.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "onchange": "^7.0.2",
    "prettier": "^2.0.5"
  },
  "license": "MIT"
}
